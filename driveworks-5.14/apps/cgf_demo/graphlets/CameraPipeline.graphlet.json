{
    "name": "CameraPipeline",
    "inputPorts": {
        "VIRTUAL_SYNC_TIME": {
            "type": "dwTime_t"
        },
        "SENSOR_EXTRINSICS": {
            "type": "dwCalibratedExtrinsics"
        }
    },
    "outputPorts": {
        "SENSOR_PROPERTIES": {
            "type": "dwSensorNodeProperties"
        },
        "CAMERA_INTRINSICS": {
            "type": "dwCameraIntrinsics"
        },
        "CUDA_ISP_YUV_UINT_8_HALF_RES": {
            "type": "dwImageHandle_t"
        },
        "FEATURE_HISTORY_CPU": {
            "type": "dwFeatureHistoryArray"
        },
        "FEATURE_HISTORY_GPU": {
            "type": "dwFeatureHistoryArray"
        },
        "IMAGE_TIMESTAMP": {
            "type": "dwTime_t"
        },
        "NEXT_IMAGE_TIMESTAMP": {
            "type": "dwTime_t"
        }
    },
    "parameters": {
        "cameraIndex": {
            "type": "size_t",
            "default": 0
        },
        "fovealMode": {
            "type": "dwFovealMode",
            "default": "OFF"
        },
        "useEEPROMIntrinsics": {
            "type": "bool",
            "default": false
        }
    },
    "subcomponents": {
        "cameraPreprocessing": {
            "componentType": "CameraPreprocessingDemo.graphlet.json",
            "parameters": {
                "cameraIndex": "$cameraIndex"
            }
        },
        "cameraSensor": {
            "componentType": "CameraSensor.graphlet.json",
            "parameters": {
                "*": "$*"
            }
        }
    },
    "connections": [
        {
            "src": "SENSOR_EXTRINSICS",
            "dests": {
                "cameraSensor.SENSOR_EXTRINSICS": {}
            }
        },
        {
            "src": "VIRTUAL_SYNC_TIME",
            "dests": {
                "cameraSensor.VIRTUAL_SYNC_TIME": {}
            }
        },
        {
            "src": "cameraPreprocessing.FEATURE_HISTORY_CPU",
            "dests": {
                "FEATURE_HISTORY_CPU": {}
            }
        },
        {
            "src": "cameraPreprocessing.FEATURE_HISTORY_GPU",
            "dests": {
                "FEATURE_HISTORY_GPU": {}
            }
        },
        {
            "src": "cameraSensor.CUDA_ISP_YUV_UINT_8_HALF_RES",
            "dests": {
                "CUDA_ISP_YUV_UINT_8_HALF_RES": {},
                "cameraPreprocessing.IMAGE": {}
            }
        },
        {
            "src": "cameraSensor.IMAGE_TIMESTAMP",
            "dests": {
                "IMAGE_TIMESTAMP": {}
            }
        },
        {
            "src": "cameraSensor.INTRINSICS",
            "dests": {
                "CAMERA_INTRINSICS": {}
            }
        },
        {
            "src": "cameraSensor.NEXT_IMAGE_TIMESTAMP",
            "dests": {
                "NEXT_IMAGE_TIMESTAMP": {}
            }
        },
        {
            "src": "cameraSensor.SENSOR_PROPERTIES",
            "dests": {
                "SENSOR_PROPERTIES": {}
            }
        }
    ]
}
